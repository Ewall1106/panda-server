{
  "name": "panda-server",
  "version": "2.0.0",
  "description": "使用egg实现对vue-h5-template的后端服务支持。",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-scripts": "^2.11.0",
    "nanoid": "^3.1.20",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=egg-server-project",
    "stop": "egg-scripts stop --title=egg-server-project",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "keywords": [
    "egg",
    "node",
    "mongodb",
    "mongoose"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ewall1106/panda-server"
  },
  "author": "Ewall&熊猫",
  "license": "MIT"
}
